
.home {
  padding: 20px;
}
.search-bar {
  padding: 10px;
  background-color: #f5f5f5;
  border-radius: 20px;
  margin-bottom: 20px;
}
.search-bar input {
  width: 100%;
  padding: 10px;
  border: none;
  border-radius: 20px;
  background-color: #f5f5f5;
}
.browse-area {
  display: flex;
  flex-direction: column;
}
.row {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20px;
}
.col {
  flex: 1;
  margin: 0 10px;
}
.square {
  width: 100%;
  height: 100px;
  background-color: #f5f5f5;
  border-radius: 10px;
}
.nav-bar {
  display: flex;
  justify-content: space-between;
  padding: 10px;
  background-color: #f5f5f5;
  border-radius: 10px;
  margin-top: 20px;
}
.nav-item {
  flex: 1;
  text-align: center;
}
