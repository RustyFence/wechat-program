
 .uni-tabbar {
  display: none !important;
}
.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: #fff;
  display: flex;
  border-top: 1px solid #eee;
  z-index: 999;
}
.tab-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #666;
}
.tab-item.active {
  color: #007AFF;
}
.icon-wrapper {
  position: relative;
  display: inline-block;
  margin-bottom: 4rpx;
}
.tab-item text {
  margin-top: 4rpx;
}
